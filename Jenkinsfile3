node{
    try{
        echo "starting containers"
        sh "sudo docker start -d --name=redis redis"
        sh "sudo docker start -d --name=influx influxdb"
        sh "sudo docker start -d --name=hello-world hello-world"
    }
    catch(Exception exp){
        println "Exception occured"
        println exp.getMessage()
        println exp.getCause()
    }
}
